{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "Keycloak": {
    "Realm": "master",
    "AuthServerUrl": "http://localhost:8080/",
    "SslRequired": "external",
    "Resource": "master",
    "PublicClient": true,
    "ConfidentialPort": 0,
    "VerifyTokenAudience": false
  },

  "Mongo": {
    "Address": "mongodb://localhost:27017",
    "Database": "UrlShortner"
  },

  "Redis": {
    "ConnectionString": "localhost:6379"
  }
}
